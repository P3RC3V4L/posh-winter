{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "defaultFG": "#cccccc",
    "defaultBG": "#0c0c0c",
    "yellow": "#ffeb95",
    "blue": "#007ACC",
    "orange": "#e95420",
    "white": "#ffffff",
    "plum": "#772953",
    "midGray": "#aaaaaa",
    "palegreen": "#6ad865",
    "paleblue": "#329ee6",
    "light": "#d6deeb",
    "dark": "#575656",
    "night": "#1a1a1a",
    "rootBG": "#ff0000",
    "rootFG": "p:white",
    "ok": "#16c60c",
    "warning": "#dc9300",
    "error": "#e74856",
    "redNotice": "p:error",
    "dev": "p:palegreen",
    "staging": "p:paleblue",
    "uat": "p:yellow",
    "prod": "p:redNotice",

    "osBG": "p:defaultBG",
    "osFG": "p:defaultFG",
    "pathBG": "p:defaultBG",
    "pathSeparator": "p:midGray",
    "pathFG": "p:defaultFG",
    "memoryBG": "p:defaultBG"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:dark",
          "foreground_templates": [
            "{{ if eq .Env.P6_ENV \"prod\" }}p:prod{{ end }}",
            "{{ if eq .Env.P6_ENV \"uat\" }}p:uat{{ end }}",
            "{{ if eq .Env.P6_ENV \"staging\" }}p:staging{{ end }}",
            "{{ if eq .Env.P6_ENV \"dev\" }}p:dev{{ end }}"
          ],
          "style": "plain",
          "template": "{{ if .Env.P6_ENV }}    </>{{ end }}\u256d\u2500",
          "type": "text"
        },
        {
          "background": "p:osBG",
          "background_templates": [
            "{{ if eq .Env.P6_ENV \"prod\" }}p:prod{{ end }}",
            "{{ if eq .Env.P6_ENV \"uat\" }}p:uat{{ end }}",
            "{{ if eq .Env.P6_ENV \"staging\" }}p:staging{{ end }}",
            "{{ if eq .Env.P6_ENV \"dev\" }}p:dev{{ end }}"
          ],
          "foreground": "p:osFG",
          "properties": {
            "windows": "\ue62a"
          },
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} <p:dark>\u232a</>",
          "type": "os"
        }
,
        {
          "background": "p:rootBG",
          "foreground": "p:rootFG",
          "powerline_symbol": "\ue0bc",
          "style": "plain",
          "template": "\uf132",
          "type": "root"
        },
        {
          "background": "p:pathBG",
          "foreground": "p:pathSeparator",
          "properties": {
            "home_icon": "<p:white>\ueb06</> ",
            "style": "full",
            "mapped_locations": {
              "~/git/*": "<p:pathFG>\ue708</> "
            },
            "right_format": "<p:white>%s</>",
            "folder_separator_icon": "/"
          },
          "style": "plain",
          "template": "{{ .Path }} <p:dark>\u232a</>",
          "type": "path"
        },
        {
          "foreground": "#addb67",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#c792ea{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "plain",
          "template": "{{ url .HEAD .UpstreamURL }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:defaultBG",
          "foreground": "p:defaultFG",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<p:dark>[</>{{ .FormattedMs }}<p:dark>]</>",
          "type": "executiontime"
        },
        {
          "foreground_templates": [
            "{{ if eq .Code 0 }}p:dark{{ end }}",
            "{{ if gt .Code 0 }}p:warning{{ end }}",
            "{{ if lt .Code 0 }}p:error{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if eq .Code 0 }}\u2576{{ else }} {{ .Code }} {{ end }}",
          "type": "exit"
        }

      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "break",
      "filler": "<,transparent>\u2500</>",
      "segments": [
        {
          "background": "p:osBG",
          "foreground": "p:osFG",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ .Name }} \uf489 ",
          "type": "shell"
        },
        {
          "background": "p:memoryBG",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ round .PhysicalPercentUsed .Precision }}% \ue266 ",
          "type": "sysinfo"
        },
        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue753 ",
          "type": "angular"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad ",
          "type": "aws"
        },
        {
          "background": "#ffffff",
          "foreground": "#337bb6",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ .EnvironmentName }} \uFD03 ",
          "type": "az"
        },
        {
          "background": "#ffffff",
          "foreground": "#5398c2",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf104<#f5bf45>\uf0e7</>\uf105 ",
          "type": "azfunc"
        },
        {
          "background": "#5a7a94",
          "foreground": "#100e23",
          "style": "plain",
          "template": "<p:dark>\u2329</>  cds {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue311 ",
          "type": "cds"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uE370 ",
          "type": "crystal"
        },
        {
          "background": "#d2d2d2",
          "foreground": "#01a300",
          "style": "plain",
          "template": "<p:dark>\u2329</><#2829b2>\ue61e</> <#be1818>\ue61d</>  cmake {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "cmake"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue798 ",
          "type": "dart"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "style": "plain",
          "template": "<p:dark>\u2329</>  {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} \ue77f ",
          "type": "dotnet"
        },
        {
          "background": "#06A4CE",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue28e ",
          "type": "flutter"
        },
        {
          "background": "#ffffff",
          "foreground": "#06aad5",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue626 ",
          "type": "go"
        },
        {
          "background": "#100e23",
          "foreground": "#906cff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue61f ",
          "type": "haskell"
        },
        {
          "background": "#ffffff",
          "foreground": "#ec2729",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue738 ",
          "type": "java"
        },
        {
          "background": "#945bb3",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</><#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "julia"
        },
        {
          "background": "#906cff",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</>K {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "kotlin"
        },
        {
          "background": "#316ce4",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} \uf308 ",
          "type": "kubectl"
        },
        {
          "background": "#ffffff",
          "foreground": "#000081",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue620 ",
          "type": "lua"
        },
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#cc3a3a>\ue71e</> ",
            "yarn_icon": "<#348cba>\ue6a7</> "
          },
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "type": "node"
        },
        {
          "background": "#6488c0",
          "foreground": "#1e293b",
          "style": "plain",
          "template": "<p:dark>\u2329</>Nx {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "nx"
        },
        {
          "background": "#41436d",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue769 ",
          "type": "perl"
        },
        {
          "background": "#787CB5",
          "foreground": "#000000",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue73d ",
          "type": "php"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "style": "plain",
          "template": "<p:dark>\u2329</>  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}\ue235 ",
          "type": "python"
        },
        {
          "background": "#b9bbbf",
          "foreground": "#1c68bd",
          "style": "plain",
          "template": "<p:dark>\u2329</>R {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "r"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue7a8 ",
          "type": "rust"
        },
        {
          "background": "#fe562e",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue755 ",
          "type": "swift"
        },
        {
          "background": "p:pathBG",
          "foreground": "p:pathFG",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "<p:dark>\u2329</> {{ .CurrentDate | date .Format }} \ue641",
          "type": "time"
        }


      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:dark",
          "foreground_templates": [
            "{{ if .Env.P6_ENV }}black{{ end }}"
          ],
          "background_templates": [
            "{{ if eq .Env.P6_ENV \"prod\" }}p:prod{{ end }}",
            "{{ if eq .Env.P6_ENV \"uat\" }}p:uat{{ end }}",
            "{{ if eq .Env.P6_ENV \"staging\" }}p:staging{{ end }}",
            "{{ if eq .Env.P6_ENV \"dev\" }}p:dev{{ end }}"
          ],
          "style": "plain",
          "template": "{{ if eq .Env.P6_ENV \"prod\" }} PRD\u2570<p:prod,transparent>\u2574</>{{ else if eq .Env.P6_ENV \"uat\" }} UAT\u2570<p:uat,transparent>\u2574</>{{ else if eq .Env.P6_ENV \"staging\" }} STG\u2570<p:staging,transparent>\u2574</>{{ else if eq .Env.P6_ENV \"dev\" }} DEV\u2570<p:dev,transparent>\u2574</>{{ else }}\u2570\u2574{{ end }}",
          "type": "text"
        }
,
        {
          "foreground": "p:ok",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:warning{{ end }}",
            "{{ if lt .Code 0 }}p:error{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "exit"
        }

      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}{{ if .Root }} (admin){{ end }}",
  "final_space": true,
  "version": 2
}




