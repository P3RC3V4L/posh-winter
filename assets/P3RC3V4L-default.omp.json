{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#007ACC",
    "dark": "#575656",
    "defaultBG": "#0c0c0c",
    "defaultFG": "#cccccc",
    "dev": "p:palegreen",
    "error": "#e74856",
    "light": "#d6deeb",
    "memoryBG": "p:plum",
    "midGray": "#aaaaaa",
    "night": "#1a1a1a",
    "ok": "#16c60c",
    "orange": "#e95420",
    "osBG": "p:yellow",
    "osFG": "p:blue",
    "paleblue": "#329ee6",
    "palegreen": "#6ad865",
    "pathBG": "p:blue",
    "pathFG": "p:white",
    "pathSeparator": "p:night",
    "plum": "#772953",
    "prod": "p:redNotice",
    "redNotice": "p:error",
    "rootBG": "#ff0000",
    "rootFG": "p:white",
    "staging": "p:paleblue",
    "uat": "p:yellow",
    "warning": "#dc9300",
    "white": "#ffffff",
    "yellow": "#ffeb95"
  },
  "console_title_template": "{{ .Folder }}{{ if .Root }} (admin){{ end }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Env.P6_ENV }}    </>{{ end }}\u256d\u2500",
          "foreground": "p:osBG",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq .Env.P6_ENV \"prod\" }}p:prod{{ end }}",
            "{{ if eq .Env.P6_ENV \"uat\" }}p:uat{{ end }}",
            "{{ if eq .Env.P6_ENV \"staging\" }}p:staging{{ end }}",
            "{{ if eq .Env.P6_ENV \"dev\" }}p:dev{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "windows": "\ue62a"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} {{ if .Root }}<p:rootBG>\uf132</>{{ end }}",
          "foreground": "p:osFG",
          "background": "p:osBG",
          "type": "os",
          "style": "diamond",
          "background_templates": [
            "{{ if eq .Env.P6_ENV \"prod\" }}p:prod{{ end }}",
            "{{ if eq .Env.P6_ENV \"uat\" }}p:uat{{ end }}",
            "{{ if eq .Env.P6_ENV \"staging\" }}p:staging{{ end }}",
            "{{ if eq .Env.P6_ENV \"dev\" }}p:dev{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "cycle": [
              "p:pathFG"
            ],
            "folder_separator_icon": "<b>\\</b>",
            "home_icon": "<p:pathFG>\ueb06</> ",
            "mapped_locations": {
              "C:/Windows/*": "<p:pathFG>\ue62a</> ",
              "~/git/Repos/*": "<p:pathFG>\uf09b</> "
            },
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:pathSeparator",
          "powerline_symbol": "\ue0b8",
          "background": "p:pathBG",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "trailing_diamond": "\ue0b8",
          "template": " {{ url .HEAD .UpstreamURL }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b8",
          "background": "#addb67",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#c792ea{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "leading_diamond": "\ue0be",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .FormattedMs }}",
          "foreground": "p:dark",
          "background": "p:night",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .Code }}",
          "foreground": "p:white",
          "background": "p:ok",
          "type": "exit",
          "style": "diamond",
          "foreground_templates": [
            "{{ if eq .Code 0 }}p:ok{{ end }}",
            "{{ if gt .Code 0 }}p:white{{ end }}",
            "{{ if lt .Code 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if eq .Code 0 }}p:night{{ end }}",
            "{{ if gt .Code 0 }}p:warning{{ end }}",
            "{{ if lt .Code 0 }}p:error{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0bc",
          "template": "\uf489 {{ .Name }} ",
          "foreground": "p:osFG",
          "background": "p:osBG",
          "type": "shell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "precision": 0
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ue266 {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#ffffff",
          "background": "p:memoryBG",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ce092f",
          "background": "#ffffff",
          "type": "angular",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "foreground": "#faa029",
          "background": "#565656",
          "type": "aws",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ufd03 {{ .EnvironmentName }} ",
          "foreground": "#337bb6",
          "background": "#ffffff",
          "type": "az",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#5398c2",
          "background": "#ffffff",
          "type": "azfunc",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
