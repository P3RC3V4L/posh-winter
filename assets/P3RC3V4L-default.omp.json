          "foreground": "#100e23",
          "background": "#5a7a94",
          "type": "cds",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ue370 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#000000",
          "background": "#ffffff",
