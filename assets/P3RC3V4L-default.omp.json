          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#055b9c",
          "background": "#e1e8e9",
          "type": "dart",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#0d6da8",
          "background": "#0e0e0e",
          "type": "dotnet",
